<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name>User Information Servlet</display-name>
  <description>SER422 Lab2 Task1</description>
  <servlet>
    <servlet-name>UserInfoServlet</servlet-name>
    <servlet-class>edu.asupoly.ser422.info.UserInfoServlet</servlet-class>
    <init-param>
      <param-name>userinfobook</param-name>
      <param-value>/userinfobook.xml</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>UserInfoSearchServlet</servlet-name>
    <servlet-class>edu.asupoly.ser422.info.UserInfoSearchServlet</servlet-class>
    <init-param>
      <param-name>userinfobook</param-name>
      <param-value>/userinfobook.xml</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>HomeServlet</servlet-name>
    <servlet-class>edu.asupoly.ser422.info.HomeServlet</servlet-class>
    <init-param>
      <param-name>userinfobook</param-name>
      <param-value>/userinfobook.xml</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
    <servlet>
    <servlet-name>SessionServlet</servlet-name>
    <servlet-class>edu.asupoly.ser422.info.SessionServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
      <servlet>
    <servlet-name>ErrorHandlerServlet</servlet-name>
    <servlet-class>edu.asupoly.ser422.info.ErrorHandlerServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>MultiForm1Servlet</servlet-name>
    <servlet-class>edu.asupoly.ser422.info.MultiForm1Servlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
   <servlet>
    <servlet-name>MultiForm2Servlet</servlet-name>
    <servlet-class>edu.asupoly.ser422.info.MultiForm2Servlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
   <servlet>
    <servlet-name>MultiForm3Servlet</servlet-name>
    <servlet-class>edu.asupoly.ser422.info.MultiForm3Servlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
     <servlet>
    <servlet-name>MultiForm4Servlet</servlet-name>
    <servlet-class>edu.asupoly.ser422.info.MultiForm4Servlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
     <servlet>
    <servlet-name>MultiForm5Servlet</servlet-name>
    <servlet-class>edu.asupoly.ser422.info.MultiForm5Servlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
       <servlet>
    <servlet-name>MultiForm6Servlet</servlet-name>
    <servlet-class>edu.asupoly.ser422.info.MultiForm6Servlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
         <servlet>
    <servlet-name>SessionRemoveServlet</servlet-name>
    <servlet-class>edu.asupoly.ser422.info.SessionRemoveServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserInfoServlet</servlet-name>
    <url-pattern>/post_coder</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserInfoSearchServlet</servlet-name>
    <url-pattern>/get_coders</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>HomeServlet</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
      <servlet-mapping>
    <servlet-name>SessionServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ErrorHandlerServlet</servlet-name>
    <url-pattern>/error</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>MultiForm1Servlet</servlet-name>
    <url-pattern>/multiform1</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>MultiForm2Servlet</servlet-name>
    <url-pattern>/multiform2</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>MultiForm3Servlet</servlet-name>
    <url-pattern>/multiform3</url-pattern>
  </servlet-mapping>
      <servlet-mapping>
    <servlet-name>MultiForm4Servlet</servlet-name>
    <url-pattern>/multiform4</url-pattern>
  </servlet-mapping>
      <servlet-mapping>
    <servlet-name>MultiForm5Servlet</servlet-name>
    <url-pattern>/multiform5</url-pattern>
  </servlet-mapping>
        <servlet-mapping>
    <servlet-name>MultiForm6Servlet</servlet-name>
    <url-pattern>/multiform6</url-pattern>
  </servlet-mapping>
          <servlet-mapping>
    <servlet-name>SessionRemoveServlet</servlet-name>
    <url-pattern>/remove</url-pattern>
  </servlet-mapping>
<error-page>
  <error-code>404</error-code>
  <location>/error</location>
</error-page>
<error-page>
  <error-code>403</error-code>
  <location>/error</location>
</error-page>
<error-page>
  <error-code>405</error-code>
  <location>/error</location>
</error-page>
<error-page>
  <error-code>500</error-code>
  <location>/error</location>
</error-page>
<error-page>
  <error-code>503</error-code>
  <location>/error</location>
</error-page>
<error-page>
  <exception-type>javax.servlet.ServletException</exception-type>
    <location>/error</location>
</error-page>
<error-page>
    <exception-type>java.io.IOException</exception-type >
     <location>/error</location>
</error-page>
<error-page>
    <exception-type>java.lang.Throwable</exception-type >
    <location>/ErrorHandler</location>
</error-page>
  <welcome-file-list>
    <welcome-file>home</welcome-file>
  </welcome-file-list>
</web-app>